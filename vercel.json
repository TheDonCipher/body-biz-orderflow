{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": { "distDir": "dist" }
    }
  ],
  "routes": [
    {
      "src": "/api/submit-purchase-order",
      "dest": "supabase/functions/submit-purchase-order/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}
